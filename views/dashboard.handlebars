<header>
      <h1>
        <a href="/" id="title">Your Dashboard</a>
      </h1>
</header>

<!-- Loop over each object in your blogs array -->

    <div class="row mb-4 posts">
      <div class="col-md-5 homepage">
        {{#each post as |post| }}
        <h2>
          {{post.title}}</a><span>: Posted by {{post.username}} on {{post.date_created}}</span>
        </h2>
        <p>{{post.content}}</p>
        <div class="col-md-4">
          <button class="btn btn-sm btn-danger deleteBtn" data-id="{{post.id}}">DELETE</button>
      </div>
      </div>
    </div>
{{/each}}

    <div class="form-group">
      <button class="btn btn-secondary" type="button" id="plusNewPost" onclick="plusNewPost()">+ New Post</button>
    </div>

<script src="../public/js/buttons.js"></script> 
<script src="../public/js/login.js"></script>
<script src="../public/js/logout.js"></script>
<script src="../public/js/plusNewPost.js"></script>
<script>
  function plusNewPost() {
  document.location.replace("/yourDashboardAdd");
}</script>