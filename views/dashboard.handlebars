<header>
      <h1>
        <a href="/" id="title">Your Dashboard</a>
      </h1>
</header>

<!-- Loop over each object in your blogs array -->

    <div class="row mb-4 posts">
      <div class="col-md-5 homepage">
        <h2>
          <!-- Call helper function with triple brackets to render the emoji span as HTML -->
          <a href="/posts/{{user.id}}">{{user.title}}</a><span>: Posted by {{user.username}} on {{date_created}}</span>
        </h2>
        <p>{{allposts.content}}</p>
      </div>
    </div>


    <div class="form-group">
      <button class="btn btn-secondary" type="submit" id="plusNewPost">+ New Post</button>
    </div>